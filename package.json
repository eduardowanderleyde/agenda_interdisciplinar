  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "7.3.0",
    "@rails/ujs": "^7.1.3-4",
    "@rails/webpacker": "5.4.4",
    "sortablejs": "^1.15.6",
    "tailwindcss": "^3.4.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  }
}
